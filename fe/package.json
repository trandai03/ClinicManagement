{
  "name": "testdatatable",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "clean": "del-cli -f '../src/main/resources/static/*' '../src/main/resources/templates/index.html'",
    "create-dirs": "mkdirp ../src/main/resources/static ../src/main/resources/templates -m 755",
    "copy-artifacts": "ncp dist/testdatatable/favicon.ico ../src/main/resources/static && ncp dist/testdatatable ../src/main/resources/static && cpx \"dist/testdatatable/index.html\" \"../src/main/resources/templates\" && del-cli -f '../src/main/resources/static/index.html'",
    "deploy": "npm-run-all build clean create-dirs copy-artifacts"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^16.2.12",
    "@angular/cdk": "^16.2.14",
    "@angular/common": "^16.2.0",
    "@angular/compiler": "^16.2.0",
    "@angular/core": "^16.2.0",
    "@angular/forms": "^16.2.0",
    "@angular/material": "^16.2.14",
    "@angular/platform-browser": "^16.2.0",
    "@angular/platform-browser-dynamic": "^16.2.0",
    "@angular/router": "^16.2.0",
    "@kurkle/color": "^0.3.2",
    "angular-datatables": "^16.0.1",
    "chart.js": "^4.4.2",
    "datatables.net": "^1.11.3",
    "datatables.net-dt": "^1.11.3",
    "jquery": "^3.6.0",
    "ngx-pagination": "^6.0.2",
    "ngx-toastr": "^19.0.0",
    "primeng": "^16.9.7-lts",
    "rxjs": "~7.8.0",
    "simple-datatables": "^9.0.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.13.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^16.2.12",
    "@angular/cli": "^16.2.12",
    "@angular/compiler-cli": "^16.2.0",
    "@types/datatables.net": "^1.10.21",
    "@types/jasmine": "~4.3.0",
    "@types/jquery": "^3.5.9",
    "cpx": "^1.5.0",
    "del-cli": "^6.0.0",
    "jasmine-core": "~4.6.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "ncp": "^2.0.0",
    "npm-run-all": "^4.1.5",
    "typescript": "~5.1.3"
  }
}
